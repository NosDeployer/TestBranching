<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="mForm5.master.cs" Inherits="LiquiForce.LFSLive.WebUI.mForm5" %>
<%@ Register Assembly="AjaxControlToolkit" Namespace="AjaxControlToolkit" TagPrefix="ajax" %>
<%@ Register Assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" Namespace="System.Web.UI" TagPrefix="asp" %> 

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head id="HeadMForm5" runat="server">
    <meta content='text/html; charset=UTF-8' http-equiv='Content-Type'/>
    <meta http-equiv="X-UA-Compatible" content="IE=8"/>
    <title>Untitled Page</title> 
</head>
<body id="myBody" runat="server" style="margin: 0px;"  class="MasterPage_Background" >
    <form id="form1" runat="server">
